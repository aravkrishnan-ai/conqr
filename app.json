{
  "expo": {
    "name": "Conqr",
    "slug": "conqr",
    "scheme": "conqr",
    "owner": "aravconqr",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aravkrishnan.conqr",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Conqr needs your location to track your movements and claim territories.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Conqr needs your location to track your movements and claim territories even when the app is in the background."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.aravkrishnan.conqr",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "conqr",
              "host": "auth",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "plugins": [
      "expo-web-browser",
      "expo-location"
    ],
    "extra": {
      "eas": {
        "projectId": "75ecf103-6cba-4b61-b280-0aaf3276695d"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/75ecf103-6cba-4b61-b280-0aaf3276695d"
    }
  }
}
